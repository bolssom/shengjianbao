<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background" >
        <nine-patch android:src="@drawable/gressbar"/>
    </item>


    <item android:id="@android:id/progress">

        <clip>

            <shape>

                <corners android:radius="5dip" />

                <gradient
                    android:angle="270"
                    android:endColor="#e9a811"
                    android:centerY="0.75"
                    android:startColor="#ffca36" />

                <padding
                    android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            </shape>
        </clip>
    </item>

</layer-list>